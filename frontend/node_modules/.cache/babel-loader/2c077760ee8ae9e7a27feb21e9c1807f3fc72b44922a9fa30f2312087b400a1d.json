{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'LkPage',\n  data() {\n    return {\n      userName: '',\n      userEmail: '',\n      joinDate: '',\n      completedTasks: 0,\n      daysActive: 0,\n      achievements: 0,\n      avatar: ''\n    };\n  },\n  async created() {\n    const token = localStorage.getItem('token');\n    if (token) {\n      try {\n        const response = await api.getUserInfo(token);\n        this.userName = response.data.username;\n        this.userEmail = response.data.email;\n        this.joinDate = new Date(response.data.created_at).toLocaleDateString();\n        // Дополнительные данные можно добавить здесь\n      } catch (error) {\n        alert('Ошибка при получении информации о пользователе: ' + error.response.data.detail);\n      }\n    } else {\n      this.$router.push('/login');\n    }\n  },\n  methods: {\n    changePassword() {\n      alert('Функция изменения пароля в разработке.');\n    },\n    updateEmail() {\n      alert('Функция изменения email в разработке.');\n    },\n    logout() {\n      alert('Вы вышли из аккаунта.');\n      // Здесь можно добавить логику выхода из аккаунта\n    }\n  }\n};","map":{"version":3,"names":["name","data","userName","userEmail","joinDate","completedTasks","daysActive","achievements","avatar","created","token","localStorage","getItem","response","api","getUserInfo","username","email","Date","created_at","toLocaleDateString","error","alert","detail","$router","push","methods","changePassword","updateEmail","logout"],"sources":["F:\\_MentalHealthSecond\\frontend\\src\\components\\LkPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"lk-page\">\r\n    <!-- Заголовок страницы -->\r\n    <header class=\"header\">\r\n      <h1>Личный кабинет</h1>\r\n      <p>Добро пожаловать, {{ userName }}!</p>\r\n    </header>\r\n\r\n    <!-- Основной контент -->\r\n    <main class=\"content\">\r\n      <!-- Секция с информацией о пользователе -->\r\n      <section class=\"section user-info\">\r\n        <h2>Ваши данные</h2>\r\n        <div class=\"info-container\">\r\n          <div class=\"avatar-container\">\r\n            <img\r\n              src=\"https://via.placeholder.com/150\"\r\n              alt=\"Аватар пользователя\"\r\n              class=\"avatar\"\r\n            />\r\n            <button class=\"edit-button\">Изменить фото</button>\r\n          </div>\r\n          <div class=\"details\">\r\n            <p><strong>Имя:</strong> {{ userName }}</p>\r\n            <p><strong>Email:</strong> {{ userEmail }}</p>\r\n            <p><strong>Дата регистрации:</strong> {{ joinDate }}</p>\r\n          </div>\r\n        </div>\r\n      </section>\r\n\r\n      <!-- Секция с настройками -->\r\n      <section class=\"section settings\">\r\n        <h2>Настройки</h2>\r\n        <div class=\"settings-options\">\r\n          <button class=\"settings-button\" @click=\"changePassword\">Изменить пароль</button>\r\n          <button class=\"settings-button\" @click=\"updateEmail\">Изменить email</button>\r\n          <button class=\"settings-button\" @click=\"logout\">Выйти из аккаунта</button>\r\n        </div>\r\n      </section>\r\n\r\n      <!-- Секция со статистикой -->\r\n      <section class=\"section statistics\">\r\n        <h2>Ваша активность</h2>\r\n        <div class=\"stats-container\">\r\n          <div class=\"stat-item\">\r\n            <span class=\"stat-value\">{{ completedTasks }}</span>\r\n            <span class=\"stat-label\">Завершено задач</span>\r\n          </div>\r\n          <div class=\"stat-item\">\r\n            <span class=\"stat-value\">{{ daysActive }}</span>\r\n            <span class=\"stat-label\">Дней с нами</span>\r\n          </div>\r\n          <div class=\"stat-item\">\r\n            <span class=\"stat-value\">{{ achievements }}</span>\r\n            <span class=\"stat-label\">Достижения</span>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </main>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'LkPage',\r\n  data() {\r\n    return {\r\n      userName: '',\r\n      userEmail: '',\r\n      joinDate: '',\r\n      completedTasks: 0,\r\n      daysActive: 0,\r\n      achievements: 0,\r\n      avatar: '',\r\n    };\r\n  },\r\n  async created() {\r\n    const token = localStorage.getItem('token');\r\n    if (token) {\r\n      try {\r\n        const response = await api.getUserInfo(token);\r\n        this.userName = response.data.username;\r\n        this.userEmail = response.data.email;\r\n        this.joinDate = new Date(response.data.created_at).toLocaleDateString();\r\n        // Дополнительные данные можно добавить здесь\r\n      } catch (error) {\r\n        alert('Ошибка при получении информации о пользователе: ' + error.response.data.detail);\r\n      }\r\n    } else {\r\n      this.$router.push('/login');\r\n    }\r\n  },\r\n  methods: {\r\n    changePassword() {\r\n      alert('Функция изменения пароля в разработке.');\r\n    },\r\n    updateEmail() {\r\n      alert('Функция изменения email в разработке.');\r\n    },\r\n    logout() {\r\n      alert('Вы вышли из аккаунта.');\r\n      // Здесь можно добавить логику выхода из аккаунта\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.lk-page {\r\n  font-family: Arial, sans-serif;\r\n  color: #333;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.header {\r\n  text-align: center;\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.header h1 {\r\n  font-size: 2.5rem;\r\n  color: #2c3e50;\r\n}\r\n\r\n.header p {\r\n  font-size: 1.2rem;\r\n  color: #666;\r\n}\r\n\r\n.content {\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.section {\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.section h2 {\r\n  font-size: 2rem;\r\n  color: #2c3e50;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.user-info .info-container {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 40px;\r\n}\r\n\r\n.avatar-container {\r\n  text-align: center;\r\n}\r\n\r\n.avatar {\r\n  width: 150px;\r\n  height: 150px;\r\n  border-radius: 50%;\r\n  object-fit: cover;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.edit-button {\r\n  background-color: #2c3e50;\r\n  color: white;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-size: 1rem;\r\n}\r\n\r\n.edit-button:hover {\r\n  background-color: #1a2a36;\r\n}\r\n\r\n.details p {\r\n  font-size: 1.1rem;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.settings-options {\r\n  display: flex;\r\n  gap: 20px;\r\n}\r\n\r\n.settings-button {\r\n  background-color: #2c3e50;\r\n  color: white;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-size: 1rem;\r\n}\r\n\r\n.settings-button:hover {\r\n  background-color: #1a2a36;\r\n}\r\n\r\n.statistics .stats-container {\r\n  display: flex;\r\n  justify-content: space-around;\r\n  text-align: center;\r\n}\r\n\r\n.stat-item {\r\n  flex: 1;\r\n  padding: 20px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 10px;\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.stat-value {\r\n  font-size: 2rem;\r\n  font-weight: bold;\r\n  color: #2c3e50;\r\n}\r\n\r\n.stat-label {\r\n  font-size: 1rem;\r\n  color: #666;\r\n}\r\n\r\n.footer {\r\n  text-align: center;\r\n  padding: 20px;\r\n  background-color: #f8f9fa;\r\n  border-top: 1px solid #ddd;\r\n  margin-top: 40px;\r\n}\r\n\r\n.footer p {\r\n  font-size: 0.9rem;\r\n  color: #666;\r\n}\r\n</style>"],"mappings":";AAgEA,eAAe;EACbA,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,EAAE;MACZC,cAAc,EAAE,CAAC;MACjBC,UAAU,EAAE,CAAC;MACbC,YAAY,EAAE,CAAC;MACfC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;IAC3C,IAAIF,KAAK,EAAE;MACT,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMC,GAAG,CAACC,WAAW,CAACL,KAAK,CAAC;QAC7C,IAAI,CAACR,QAAO,GAAIW,QAAQ,CAACZ,IAAI,CAACe,QAAQ;QACtC,IAAI,CAACb,SAAQ,GAAIU,QAAQ,CAACZ,IAAI,CAACgB,KAAK;QACpC,IAAI,CAACb,QAAO,GAAI,IAAIc,IAAI,CAAC