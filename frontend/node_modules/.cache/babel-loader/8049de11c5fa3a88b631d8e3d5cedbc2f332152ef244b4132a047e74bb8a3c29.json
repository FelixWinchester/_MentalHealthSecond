{"ast":null,"code":"export default {\n  name: 'RegisterPage',\n  data() {\n    return {\n      name: '',\n      // Имя пользователя\n      email: '',\n      // Email пользователя\n      password: '',\n      // Пароль пользователя\n      confirmPassword: '' // Подтверждение пароля\n    };\n  },\n  methods: {\n    handleRegister() {\n      // Логика регистрации\n      if (this.password !== this.confirmPassword) {\n        alert('Пароли не совпадают.');\n        return;\n      }\n      if (this.name && this.email && this.password) {\n        alert('Регистрация успешна!');\n        // Здесь можно добавить логику для отправки данных на сервер\n      } else {\n        alert('Пожалуйста, заполните все поля.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","email","password","confirmPassword","methods","handleRegister","alert"],"sources":["F:\\_MentalHealthSecond\\frontend\\src\\components\\RegisterPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"register-page\">\r\n    <!-- Заголовок страницы -->\r\n    <header class=\"header\">\r\n      <h1>Регистрация</h1>\r\n      <p>Создайте новый аккаунт.</p>\r\n    </header>\r\n\r\n    <!-- Форма регистрации -->\r\n    <form class=\"register-form\" @submit.prevent=\"handleRegister\">\r\n      <div class=\"form-group\">\r\n        <label for=\"name\">Имя:</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"name\"\r\n          v-model=\"name\"\r\n          placeholder=\"Введите ваше имя\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">Email:</label>\r\n        <input\r\n          type=\"email\"\r\n          id=\"email\"\r\n          v-model=\"email\"\r\n          placeholder=\"Введите ваш email\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"password\">Пароль:</label>\r\n        <input\r\n          type=\"password\"\r\n          id=\"password\"\r\n          v-model=\"password\"\r\n          placeholder=\"Введите ваш пароль\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"confirm-password\">Подтвердите пароль:</label>\r\n        <input\r\n          type=\"password\"\r\n          id=\"confirm-password\"\r\n          v-model=\"confirmPassword\"\r\n          placeholder=\"Подтвердите ваш пароль\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <button type=\"submit\" class=\"register-button\">Зарегистрироваться</button>\r\n    </form>\r\n\r\n    <!-- Дополнительные ссылки -->\r\n    <div class=\"links\">\r\n      <p>Уже есть аккаунт? <router-link to=\"/login\" class=\"link\">Войдите</router-link></p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'RegisterPage',\r\n  data() {\r\n    return {\r\n      name: '', // Имя пользователя\r\n      email: '', // Email пользователя\r\n      password: '', // Пароль пользователя\r\n      confirmPassword: '', // Подтверждение пароля\r\n    };\r\n  },\r\n  methods: {\r\n    handleRegister() {\r\n      // Логика регистрации\r\n      if (this.password !== this.confirmPassword) {\r\n        alert('Пароли не совпадают.');\r\n        return;\r\n      }\r\n\r\n      if (this.name && this.email && this.password) {\r\n        alert('Регистрация успешна!');\r\n        // Здесь можно добавить логику для отправки данных на сервер\r\n      } else {\r\n        alert('Пожалуйста, заполните все поля.');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.register-page {\r\n  font-family: Arial, sans-serif;\r\n  color: #333;\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.header {\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.header h1 {\r\n  font-size: 2.5rem;\r\n  color: #2c3e50;\r\n}\r\n\r\n.header p {\r\n  font-size: 1.1rem;\r\n  color: #666;\r\n}\r\n\r\n.register-form {\r\n  background-color: #f8f9fa;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n  text-align: left;\r\n}\r\n\r\n.form-group label {\r\n  display: block;\r\n  font-size: 1rem;\r\n  color: #444;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.form-group input {\r\n  width: 100%;\r\n  padding: 10px;\r\n  font-size: 1rem;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n  outline: none;\r\n}\r\n\r\n.form-group input:focus {\r\n  border-color: #2c3e50;\r\n}\r\n\r\n.register-button {\r\n  width: 100%;\r\n  padding: 10px;\r\n  font-size: 1.1rem;\r\n  background-color: #2c3e50;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.register-button:hover {\r\n  background-color: #1a2a36;\r\n}\r\n\r\n.links {\r\n  margin-top: 20px;\r\n}\r\n\r\n.link {\r\n  color: #2c3e50;\r\n  text-decoration: none;\r\n  font-size: 0.9rem;\r\n}\r\n\r\n.link:hover {\r\n  text-decoration: underline;\r\n}\r\n</style>"],"mappings":"AAiEA,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,IAAI,EAAE,EAAE;MAAE;MACVE,KAAK,EAAE,EAAE;MAAE;MACXC,QAAQ,EAAE,EAAE;MAAE;MACdC,eAAe,EAAE,EAAE,CAAE;IACvB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf;MACA,IAAI,IAAI,CAACH,QAAO,KAAM,IAAI,CAACC,eAAe,EAAE;QAC1CG,KAAK,CAAC,sBAAsB,CAAC;QAC7B;MACF;MAEA,IAAI,IAAI,CAACP,IAAG,IAAK,IAAI,CAACE,KAAI,IAAK,IAAI,CAACC,QAAQ,EAAE;QAC5CI,KAAK,CAAC,sBAAsB,CAAC;QAC7B;MACF,OAAO;QACLA,KAAK,CAAC,iCAAiC,CAAC;MAC1C;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}